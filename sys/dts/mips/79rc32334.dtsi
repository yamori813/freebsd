						20 24 28 2c
						30 34 ];

			interrupt-parent = <&intc>;
			interrupts = <7>;
		};
*/

	};

	pci0 {
		compatible = "idt,rc32300-pci";
		reg = <0x18002000 0x1000
			0x18800000 0x100000
			0x40000000 0x10000000
			0x60000000 0x10000000
			0x18c00000 0x400000>;
		interrupt-parent = <&intc>;
		#address-cells = <3>;
		#size-cells = <2>;
		#interrupt-cells = <1>;
		interrupt-map-mask = <0xf800 0 0 7>;
		interrupt-map = <0x2000 0 0 1 &intc 13
				0x2000 0 0 2 &intc 13
				0x2000 0 0 3 &intc 13
				0x2000 0 0 4 &intc 13>;
/*
		interrupt-controller;
		#interrupt-cells = <1>;
		interrupt-parent = <&intc>;
		interrupts = <11 12 13>;
