/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "idt,79rc32334-soc";

	aliases {
		serial0 = &serial0;
		intc = &intc;
	};

	cpus {
		cpu@0 {
			compatible = "mips,mips4KEc";
		};
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	cpuintc: cpuintc@0 {
		#address-cells = <0>;
		#interrupt-cells = <1>;
		interrupt-controller;
		compatible = "mti,cpu-interrupt-controller";
	};

	simplebus0: simplebus0@18000000 {
		compatible = "simple-bus";
		ranges = <0x0 0x18000000 0x1FFFFF>;

		#address-cells = <1>;
		#size-cells = <1>;

		serial0: serial0@800 {
			compatible = "ns16550";
			reg = <0x800 0x20>;
			clock-frequency = <75000000>;
			interrupt-parent = <&intc>;
			interrupts = <5>;
			reg-shift = <2>;
		};

		serial1: serial1@820 {
			compatible = "ns16550";
			reg = <0x820 0x20>;
			clock-frequency = <75000000>;
			interrupt-parent = <&intc>;
			interrupts = <6>;
			reg-shift = <2>;
		};

		intc: intc@0 {
			compatible = "idt,rc32300-intc";
			reg = <0x500 0x100>;

			interrupt-controller;
			#interrupt-cells = <1>;

			interrupt-parent = <&cpuintc>;
			interrupts = <3>;
		};

		memctl: memctl@0 {
			compatible = "idt,rc32300-memctl";
			reg = <0x200 0x100>;
		};
/*

