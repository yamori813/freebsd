/dts-v1/;

#include "m83261g13.dtsi"


/ {
	compatible = "E-WMTA22", "mindspeed,m831xx-soc";
	model = "E-WMTA22";
	#address-cells = <1>;
	#size-cells = <1>;

	memory {
		device_type = "memory";
		reg = <0x80000000 0x8000000>;		// 128M at 0x80000000
	};

	flash@20000000 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "cfi-flash";
		reg = <0x20000000 0x2000000>;		// 32M at 0x20000000
		bank-width = <2>;
		device-width = <1>;

		partition@0 {
			reg = <0x00000000 0x00040000>;
			label = "u-boot";
		};
		partition@1 {
			reg = <0x00040000 0x00020000>;
			label = "env";
		};
		partition@2 {
			reg = <0x00060000 0x00020000>;
			label = "data";
		};
		partition@3 {
			reg = <0x00080000 0x00020000>;
			label = "wlan";
		};
		partition@4 {
			reg = <0x000a0000 0x00100000>;
			label = "config";
		};
		partition@5 {
			reg = <0x001a0000 0x00160000>;
			label = "kernel";
		};
		partition@6 {
			reg = <0x00300000 0x00800000>;
			label = "rootfs";
		};
	};
/*
	gpio-leds {
		compatible = "gpio-leds";

		status {
			label = "status";
			gpios = <&gpio0 4 0>;
		};
	};
*/

};

