/dts-v1/;

#include "rt3050_iic.dtsi"

/ {
	compatible = "WLR300N", "ralink,rt3050-soc";
	model = "Corega WLR300N";

	cfi@1f000000 {
		compatible = "cfi-flash";
		reg = <0x1f000000 0x800000>;
		bank-width = <2>;
		device-width = <2>;
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "uBoot";
			reg = <0x0 0x60000>;
			read-only;
		};

		partition@50000 {
			label = "kernel";
			reg = <0x00060000 0x00120000>;
			read-only;
		};

		partition@160000 {
			label = "rootfs";
			reg = <0x00180000 0x00660000>;
			read-only;
		};
/*
		partition@800000 {
			label = "upgrade";
			reg = <0x00060000 0x00780000>;
		};
*/

		factory: partition@7e0000 {
			label = "NVRAM";
			reg = <0x007e0000 0x00010000>;
		};

		partition@7f0000 {
			label = "NVRAM_FACTORY";
			reg = <0x007f0000 0x00010000>;
		};
	};


	gpio-leds {
		compatible = "gpio-leds";

		status {
			label = "status";
			gpios = <&gpio0 5 0>;
		};

		led1 {
			label = "led1";
			gpios = <&gpio0 12 1>;
		};

		led2 {
			label = "led2";
			gpios = <&gpio0 6 1>;
		};

		led3 {
			label = "led3";
			gpios = <&gpio0 8 1>;
		};

		led4 {
			label = "usb";
			gpios = <&gpio0 18 1>;
		};

	};
	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 10 1>;
			linux,code = <0x198>;
		};

	};
 	rtl8366rb {
		compatible = "rtl8366rb";
/*
 		gpio-sda = <&gpio0 1 0>;
 		gpio-sck = <&gpio0 2 0>;
*/
 	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "jtag", "uartf";
			ralink,function = "gpio";
		};

		rgmii {
			ralink,group = "rgmii";
			ralink,function = "rgmii";
		};

		mdio {
			ralink,group = "mdio";
			ralink,function = "mdio";
		};
	};
};
/*
